          "query": "from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"host\"] == \"${host}\")\n  |> filter(fn: (r) => r._measurement == \"sov_hyperlane_rate_limiter_capacity\")\n  |> filter(fn: (r) => r._field == \"current_capacity\" or r._field == \"max_capacity\")\n  |> filter(fn: (r) => r[\"direction\"] == \"${direction}\" or \"${direction}\" == \"all\")\n  |> filter(fn: (r) => r[\"route_id\"] == \"${route_id}\" or \"${route_id}\" == \"all\")\n  |> aggregateWindow(every: ${agg_interval}, fn: last, createEmpty: false)\n  |> pivot(rowKey:[\"_time\", \"direction\", \"route_id\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _field: r.route_id + \" \" + (if r.direction == \"inbound\" then \"Inbound\" else if r.direction == \"outbound\" then \"Outbound\" else r.direction),\n      _value: (r.current_capacity / r.max_capacity) * 100.0\n  }))\n  |> group(columns: [\"_field\"])",
